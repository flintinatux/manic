{
  "name": "manic",
  "version": "0.0.0",
  "description": "game engine",
  "author": "flintinatux",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/flintinatux/manic.git"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "browserify -s m src/index.js | buble > dist/manic.js",
    "clean": "rm -rf dist && mkdir -p dist",
    "hint": "jshint --reporter=node_modules/jshint-stylish .",
    "postbuild": "uglifyjs dist/manic.js -c -m > dist/manic.min.js",
    "postversion": "git push --tags origin master",
    "prebuild": "yarn run clean && mkdir -p dist",
    "preversion": "git checkout master && yarn run build",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "crocks": "^0.1.0",
    "eventemitter3": "^2.0.2",
    "keycode": "^2.1.8"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "buble": "^0.15.1",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "uglify-js": "^2.7.5"
  }
}
